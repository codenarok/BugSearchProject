{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "mimeTypes": {
    ".js": "application/javascript",
    ".jsx": "text/jsx"
  },
  "globalHeaders": {
    "Cache-Control": "no-store, must-revalidate, no-cache, max-age=0",
    "X-Content-Type-Options": "nosniff"
  }
}
